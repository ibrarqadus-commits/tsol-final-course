{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/admin/login",
      "dest": "/api/index.js"
    },
    {
      "src": "/admin/login/auth",
      "dest": "/api/index.js"
    },
    {
      "src": "/student/login",
      "dest": "/api/index.js"
    },
    {
      "src": "/student/login/auth",
      "dest": "/api/index.js"
    },
    {
      "src": "/auth/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/admin.html",
      "dest": "/api/index.js"
    },
    {
      "src": "/health",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*\\.(html|css|js|json|svg|jpg|jpeg|png|gif|ico|pdf))",
      "dest": "/$1",
      "headers": {
        "Cache-Control": "public, max-age=3600"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}

