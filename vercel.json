{
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/admin/login",
      "destination": "/api/index.js"
    },
    {
      "source": "/admin/login/auth",
      "destination": "/api/index.js"
    },
    {
      "source": "/student/login",
      "destination": "/api/index.js"
    },
    {
      "source": "/student/login/auth",
      "destination": "/api/index.js"
    },
    {
      "source": "/auth/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/admin.html",
      "destination": "/api/index.js"
    },
    {
      "source": "/health",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index.js"
    }
  ]
}

